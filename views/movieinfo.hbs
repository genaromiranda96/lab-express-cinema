<div >
    <img src='{{movie.image}}' alt='{{movie.title}}'>
    <a href='/movies'>Go Back</a>
    <h2>{{movie.title}}</h2>
    <h3>Director: {{movie.director}}</h3>
    <p>Stars</p>
    <ul>
        {{#each movie.stars}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <p>{{movie.description}}</p>
    <p>
        {{#each movie.showtimes}}
        <span>{{this}}</span>
        {{/each}}
    </p>
</div>